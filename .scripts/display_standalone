#!/bin/zsh
xrandr --output DP-1 --off \
  --output HDMI-1 --off \
  --output eDP-1 --primary --scale-from 1366x768 --mode 1366x768 --rotate normal \
  --output HDMI-2 --off \
  && notify-send "On the move!" || notify-send "unable to undock :("
xset s 90
xset dpms 90 90 90
xset dpms force on
remapkeyboard
